{
  "name": "pragma-lab",
  "version": "0.1.0",
  "private": true,
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint --fix",
    "lint-staged": "lint-staged",
    "format": "prettier --write \"**/*.{ts, tsx,js,jsx,md,mdx,css,yaml,yml}\"",
    "check": "prettier --check \"**/*.{ts, tsx,js,jsx,md,mdx,css,yaml,yml}\"",
    "prepare": "[ -n \"$CI\" ] && echo 'Skipping husky in CI environment' || npx husky",
    "migrate": "dotenv -e .env.development.local -- npx prisma migrate dev",
    "seed": "dotenv -e .env.development.local -- npx prisma db seed",
    "generate": "dotenv -e .env.development.local -- npx prisma generate",
    "prisma": "dotenv -e .env.development.local npx prisma db pull && npx prisma generate",
    "push": "dotenv -e .env.development.local -- npx prisma db push"
  },
  "dependencies": {
